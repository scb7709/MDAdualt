<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android_custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"

    android:layout_height="match_parent">
<RelativeLayout
    android:id="@+id/view"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/roundProgressBar4"
        android:layout_width="68dip"

        android:layout_height="68dip"
        android:layout_below="@+id/pingji"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginTop="6dp"
      >


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ll"
            android:textSize="32dp"
            android:textColor="#42c3f7"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>
    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="总大卡"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/roundProgressBar4"
        android:textSize="13dp"
        android:textColor="#3c3c3c" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="日均大卡"
        android:layout_alignTop="@+id/textView34"
        android:layout_toStartOf="@+id/roundProgressBar4"
        android:textColor="#3c3c3c"
        android:textSize="13dp" />

    <TextView
        android:id="@+id/TotalCal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textColor="#000000"

        android:textSize="24dp"
        android:layout_below="@+id/textView34"
        android:layout_alignStart="@+id/textView34"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/AvgCal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="0"
        android:textColor="#000000"
        android:textSize="24dp"
        android:layout_below="@+id/textView35"
        android:layout_alignStart="@+id/textView35"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/pingji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:text="非常好，继续坚持"
        android:textSize="18dp"
        android:textColor="#3c3c3c" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="过去7天"
        android:textColor="#c7c7c7"
        android:textSize="13dp"
        android:layout_above="@+id/MaxTotalTime"
        android:layout_alignEnd="@+id/textView41" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="342dp"
        android:layout_height="0.5dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="#c7c7c7" />

    <TextView
        android:id="@+id/MaxTotalTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView41"
        android:layout_alignStart="@+id/textView41"
        android:text="0"
        android:textColor="#c7c7c7"
        android:textSize="13dp" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/zhu"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/textView41"
        >



        <TextView
            android:id="@+id/midleTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"


            android:textColor="#c7c7c7"
            android:textSize="13dp"
            android:layout_above="@+id/textView43"
            android:layout_alignStart="@+id/textView43" />

        <TextView
            android:id="@+id/textView43"
            android:layout_width="342dp"
            android:layout_height="0.5dp"

            android:background="#c7c7c7"
            android:layout_centerVertical="true"
            android:layout_alignStart="@+id/textView45" />

        <TextView
            android:id="@+id/textView45"
            android:layout_width="342dp"
            android:layout_height="1.5dp"
            android:background="#c7c7c7"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true" />

        <FrameLayout
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:id="@+id/frameLayout3"
            android:layout_above="@+id/textView45"
            android:layout_toEndOf="@+id/midleTime">


            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:id="@+id/clzhouyi"
                android:background="#ffad00"
                android:layout_gravity="center_horizontal|bottom"
                android:layout_alignParentBottom="true"
                android:layout_toEndOf="@+id/midleTime" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:id="@+id/button6"

            android:layout_above="@+id/textView45"
            android:layout_toEndOf="@+id/frameLayout3"
            android:layout_marginLeft="20dp">


            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"

                android:id="@+id/clzhouer"
                android:background="#ffad00"
                android:layout_gravity="center_horizontal|bottom" />


        </FrameLayout>

        <FrameLayout
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:id="@+id/frameLayout"
            android:layout_above="@+id/textView45"
            android:layout_toEndOf="@+id/button6"
            android:layout_marginLeft="20dp">

            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:id="@+id/clzhousan"
                android:background="#ffad00"
                android:layout_gravity="center_horizontal|bottom" />

            <!--    <Button
                    android:layout_width="match_parent"
                    android:layout_height="87dp"
                    android:id="@+id/button9"
                    android:background="#42c3f7"
                    android:layout_gravity="right|bottom" />-->
        </FrameLayout>

        <FrameLayout
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:id="@+id/frameLayout2"
            android:layout_above="@+id/textView45"
            android:layout_toEndOf="@+id/frameLayout"
            android:layout_marginStart="21dp">

            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:id="@+id/clzhousi"
                android:background="#ffad00"
                android:layout_gravity="center_horizontal|bottom" />


        </FrameLayout>

        <FrameLayout
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:id="@+id/frameLayout4"
            android:layout_above="@+id/textView45"
            android:layout_toEndOf="@+id/frameLayout2"
            android:layout_marginLeft="20dp">

            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:id="@+id/clzhouwu"
                android:background="#ffad00"
                android:layout_gravity="left|bottom" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:id="@+id/frameLayout5"
            android:layout_above="@+id/textView45"
            android:layout_toEndOf="@+id/frameLayout4"
            android:layout_marginLeft="20dp">

            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:id="@+id/clzhouliu"
                android:background="#ffad00"
                android:layout_gravity="left|bottom" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="24dp"
            android:layout_height="match_parent"
            android:id="@+id/frameLayout6"
            android:layout_above="@+id/textView45"
            android:layout_toEndOf="@+id/frameLayout5"
            android:layout_marginLeft="20dp">

            <Button
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:id="@+id/clzhouri"
                android:background="#ffad00"
                android:layout_gravity="center_horizontal|bottom" />

        </FrameLayout>

    </RelativeLayout>

    <TextView
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:text="周一"
        android:id="@+id/t1"
        android:textSize="12dp"
        android:layout_alignTop="@+id/t2"
        android:layout_toEndOf="@+id/MaxTotalTime" />

    <TextView
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:text="周二"
        android:id="@+id/t2"
        android:layout_below="@+id/zhu"
        android:layout_toEndOf="@+id/t1"
        android:layout_marginLeft="20dp"
        android:textSize="12dp" />

    <TextView
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:text="周三"
        android:id="@+id/t3"
        android:layout_marginLeft="20dp"
        android:textSize="12dp"
        android:layout_below="@+id/zhu"
        android:layout_toEndOf="@+id/t2" />

    <TextView
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:text="周四"
        android:id="@+id/t4"
        android:textSize="12dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/zhu"
        android:layout_toEndOf="@+id/t3" />

    <TextView
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:text="周五"
        android:id="@+id/t5"
        android:textSize="12dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/zhu"
        android:layout_toEndOf="@+id/t4" />

    <TextView
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:text="周六"
        android:id="@+id/t6"
        android:textSize="12dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/zhu"
        android:layout_toEndOf="@+id/t5" />

    <TextView
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:text="周日"
        android:id="@+id/t7"
        android:textSize="12dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/zhu"
        android:layout_toEndOf="@+id/t6" />

    <TextView
        android:layout_width="225dp"
        android:layout_height="0.5dp"
        android:background="#ebebeb"
        android:id="@+id/textView8"
        android:layout_below="@+id/TotalCal"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp" />

</RelativeLayout>


</RelativeLayout>
