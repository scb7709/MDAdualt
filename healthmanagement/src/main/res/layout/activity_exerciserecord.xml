                            android:text=""
                            android:textColor="#ffffff"
                            android:textSize="17dp" />

                        <View
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_weight="1"></View>
                    </LinearLayout>

                </LinearLayout>
